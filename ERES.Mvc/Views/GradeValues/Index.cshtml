@model ERES.Mvc.Models.GradeValuesModels.StudensForGrades

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table table-striped">
    <tr>
        
        <th>
          Imię
        </th>
        <th>
           Nazwisko
        </th>

        <th></th>
    </tr>

@foreach (var item in Model.studentList) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>

        <td>
            @Html.ActionLink("Zoabcz przedmioty", "StudentRegistrations", new { id = item.StudentID })
        </td>
    </tr>
}

</table>
