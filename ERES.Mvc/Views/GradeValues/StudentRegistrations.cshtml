@model ERES.Mvc.Models.GradeValuesModels.StudentRegistrations

@{
    ViewBag.Title = "StudentRegistrations";
}

<h2>Rejestracje studenta</h2>

<table class="table table-striped">
    <tr>
        <th>Student
        </th>
        <th>Realizacja
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model.registrationList)
    {
        <tr>

            <td>
                @( item.Students.ToString())
            </td>
            <td>
                @( item.Realisations.Semesters.Name + " - " + item.Realisations.Subjects.Name)
            </td>
            <td>
                   @Html.ActionLink("Zoabcz oceny", "StudentGrades", new { id = item.RegistrationID })

            </td>
        </tr>
    }

</table>
